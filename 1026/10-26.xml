<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC '-//Sun Microsystems, Inc.//DTD Web
Application 2.3//EN' 'http://java.sun.com/dtd/web-app_2_3.dtd'>
<web-app>
<display-name>Carrito</display-name>
<description>Web carrito de la compra</description>
<context-param>
<description>Enable debugging for the application</description>
<param-name>debug</param-name>
<param-value>true</param-value>
</context-param>
<servlet>
<servlet-name>Tienda</servlet-name>
<servlet-class>Tienda</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>Tienda</servlet-name>
<url-pattern>/tienda</url-pattern>
</servlet-mapping>
<servlet>
<servlet-name>Carrito</servlet-name>
<servlet-class>Carrito</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>Carrito</servlet-name>
<url-pattern>/carrito</url-pattern>
</servlet-mapping>
<servlet>
<servlet-name>Comprar</servlet-name>
<servlet-class>Comprar</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>Comprar</servlet-name>
<url-pattern>/comprar</url-pattern>
</servlet-mapping>
<servlet>
<servlet-name>Vendedor</servlet-name>
<servlet-class>Vendedor</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>Vendedor</servlet-name>
<url-pattern>/vendedor</url-pattern>
</servlet-mapping>
</web-app>